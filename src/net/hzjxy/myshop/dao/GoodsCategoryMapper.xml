    <select id="findAllGoodsCategoryBycreatorid" resultType="net.hzjxy.myshop.entity.GoodsCategory"></select>
    <select id="sortAllGoodsCategoryT" resultType="net.hzjxy.myshop.entity.GoodsCategory"></select>
    <select id="findAllGoodsCategoryTByPage" resultType="net.hzjxy.myshop.entity.GoodsCategory"></select>
    <select id="findAllCategoryByNoTrem" resultType="net.hzjxy.myshop.entity.GoodsCategory"></select>
</mapper>